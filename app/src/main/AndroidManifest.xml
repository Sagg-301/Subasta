<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ids.appsubasta.subasta">

    <uses-feature android:name="android.hardware.camera" android:required="true" />
    <uses-feature android:name="android.permission.CAMERA"/>
    <uses-feature android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-feature android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <protected-broadcast android:name="android.intent.action.MEDIA_MOUNTED"/>

    <application
        android:name=".Aplicacion"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".LoginActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Timeline"
            android:label="@string/app_name" />
        <activity
            android:name=".Usuario.MiCuenta"
            android:label="MiCuenta" />
        <activity
            android:name=".CreacionSubasta.SubastaCreada"
            android:label="@string/app_name" />
        <activity
            android:name=".CreacionSubasta.CrearSubastaActivity"
            android:label="@string/app_name" />
        <activity
            android:name=".Bien.verLotes"
            android:label="bienes" />
        <activity
            android:name=".Registro"
            android:label="Registro" />
        <activity
            android:name=".Bien.verLotesMartillero"
            android:label="verLotesMartillero" />
        <activity android:name=".ConfiguracionActivity"></activity>

    </application>

</manifest>